rce":"HOME/task1.js","original":{"line":66,"column":15}},{"name":"attributes","generated":{"line":61,"column":16},"source":"HOME/task1.js","original":{"line":66,"column":16}},{"generated":{"line":61,"column":26},"source":"HOME/task1.js","original":{"line":66,"column":4}},{"generated":{"line":61,"column":27},"source":"HOME/task1.js","original":{"line":66,"column":27}},{"generated":{"line":61,"column":28},"source":"HOME/task1.js","original":{"line":66,"column":4}},{"name":"value","generated":{"line":61,"column":30},"source":"HOME/task1.js","original":{"line":66,"column":30}},{"generated":{"line":61,"column":35},"source":"HOME/task1.js","original":{"line":66,"column":4}},{"name":"style","generated":{"line":61,"column":38},"source":"HOME/task1.js","original":{"line":67,"column":6}},{"generated":{"line":61,"column":43},"source":"HOME/task1.js","original":{"line":67,"column":11}},{"generated":{"line":61,"column":48},"source":"HOME/task1.js","original":{"line":67,"column":16}},{"generated":{"line":61,"column":62},"source":"HOME/task1.js","original":{"line":67,"column":6}},{"generated":{"line":61,"column":65},"source":"HOME/task1.js","original":{"line":67,"column":33}},{"generated":{"line":61,"column":80},"source":"HOME/task1.js","original":{"line":67,"column":6}},{"generated":{"line":61,"column":83},"source":"HOME/task1.js","original":{"line":67,"column":51}},{"generated":{"line":61,"column":97},"source":"HOME/task1.js","original":{"line":66,"column":4}},{"generated":{"line":62,"column":0},"source":"HOME/task1.js","original":{"line":68,"column":3}},{"generated":{"line":63,"column":0},"source":"HOME/task1.js","original":{"line":69,"column":1}}],"sources":{"HOME/task1.js":"/* \n    Вывести на странице 10 постов с ресурса \n    https://jsonplaceholder.typicode.com/posts\n\n    Выводить \n    - заголовок поста\n   - ссылку на автора этого поста (class присвоить author-link) \n\n    При клике на ссылку автора поста вывести под ссылкой \n     - имя user\n     - количество постов этого user      \n    */\n\nconst posts = document.getElementById(\"posts\");\nconst URL = \"https://jsonplaceholder.typicode.com/\";\n\nfunction queryApi(endpoint) {\n  return fetch(URL + endpoint).then((r) =>\n    r.ok ? r.json() : Promise.reject(\"unseccessfull request\")\n  );\n}\n\nPromise.all([queryApi(\"posts\"), queryApi(\"users\")])\n  .then(([postsArr, usersArr]) => {\n    let postsCounterOb = {};\n\n    postsArr.forEach(({ userId }) => {\n      !postsCounterOb[userId] && (postsCounterOb[userId] = 0);\n      postsCounterOb[userId]++;\n    });\n\n    postsArr.forEach(\n      (post, idx) => !(idx % 10) && showPost(post, usersArr, postsCounterOb)\n    );\n  })\n  .catch((err) => {\n    console.log(err);\n    posts.textContent = \"Error. Try later\";\n  })\n  .finally(() => {\n    console.log(\"FINAL\");\n  });\n\nfunction showPost({ userId, title }, usersArr, countsOfPosts) {\n  let count = countsOfPosts[userId];\n  let name = usersArr[userId - 1].name;\n\n  return (posts.innerHTML += `\n    <div style=\"border-bottom: 1px solid grey; padding-bottom:20px\">\n        <p style=\"margin:5px\">Title: \"${title}\"</p>\n        <a class=\"author-link\" href=\"#\">show name of author</a>\n        <div style=\"display:none\" >\n            Author: ${name}<br>\n            Posts: ${count}\n        </div>\n    </div>`);\n}\n\ndocument.addEventListener(\"click\", handleAuthor);\n\nfunction handleAuthor({ target }) {\n  if (target.className === \"author-link\") {\n    let hiddenBlock = target.nextElementSibling;\n    style = hiddenBlock.attributes[0].value;\n\n    hiddenBlock.attributes[0].value =\n      style === \"display:none\" ? \"display:block\" : \"display:none\";\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"ebeef6ed4c757b7b0d26d245ba6e414a","cacheData":{"env":{}}}